{
    "builds": [
      {
        "src": "api/contact.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/contact", "dest": "/api/contact.js" },
      { "src": "/(.*)", "dest": "/dist/$1" }
    ]
  }
  